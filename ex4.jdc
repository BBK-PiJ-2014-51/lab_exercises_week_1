println("Enter a number: ");
String str = System.console().readLine();
int num = Integer.parseInt(str);
boolean isPrime = true;
for (int i = num -1; i > 1; i--){
	if (num % i == 0){
		isPrime = false;
	}
}
if (isPrime){
	println(str + " is a prime number.");
} else {
println(str + " is NOT a prime number.");
}
